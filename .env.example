# Application Configuration
APP_ENVIRONMENT=development
APP_DEBUG=true
APP_LOG_LEVEL=INFO
APP_HOST=0.0.0.0
APP_PORT=8000

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# AWS S3 Configuration
AWS_S3_BUCKET_RAW=concert-data-raw
AWS_S3_BUCKET_PROCESSED=concert-data-processed

# AWS Redshift Configuration
AWS_REDSHIFT_CLUSTER_IDENTIFIER=concert-data-warehouse
AWS_REDSHIFT_DATABASE=concerts
AWS_REDSHIFT_USER=your_redshift_user
AWS_REDSHIFT_PASSWORD=your_redshift_password
AWS_REDSHIFT_HOST=your-cluster.region.redshift.amazonaws.com
AWS_REDSHIFT_PORT=5439

# AWS Kinesis Configuration
AWS_KINESIS_STREAM_NAME=concert-data-stream
AWS_KINESIS_SHARD_COUNT=1

# AWS SageMaker Configuration
AWS_SAGEMAKER_EXECUTION_ROLE=arn:aws:iam::account:role/SageMakerExecutionRole
AWS_SAGEMAKER_MODEL_BUCKET=concert-ml-models

# AWS Lake Formation Configuration
AWS_LAKEFORMATION_ADMIN_ROLE=arn:aws:iam::account:role/LakeFormationAdminRole

# External API Configuration
API_SPOTIFY_CLIENT_ID=your_spotify_client_id
API_SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

API_TICKETMASTER_API_KEY=your_ticketmaster_api_key

API_MUSICBRAINZ_USER_AGENT=ConcertDataPlatform/1.0 (your-email@example.com)

# API Rate Limiting
API_RATE_LIMIT_REQUESTS=100
API_RATE_LIMIT_WINDOW=60
API_RETRY_ATTEMPTS=3
API_RETRY_BACKOFF=1.0

# AgentCore Configuration
AGENTCORE_RUNTIME_ENDPOINT=https://your-agentcore-runtime-endpoint
AGENTCORE_MEMORY_ENDPOINT=https://your-agentcore-memory-endpoint
AGENTCORE_CODE_INTERPRETER_ENDPOINT=https://your-agentcore-code-interpreter-endpoint
AGENTCORE_BROWSER_ENDPOINT=https://your-agentcore-browser-endpoint
AGENTCORE_GATEWAY_ENDPOINT=https://your-agentcore-gateway-endpoint
AGENTCORE_API_KEY=your_agentcore_api_key
AGENTCORE_REGION=us-east-1

# Database Configuration (for local development)
DB_POSTGRES_HOST=localhost
DB_POSTGRES_PORT=5432
DB_POSTGRES_DATABASE=concerts_dev
DB_POSTGRES_USER=postgres
DB_POSTGRES_PASSWORD=password
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20